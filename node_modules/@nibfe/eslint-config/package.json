{
  "name": "@nibfe/eslint-config",
  "version": "0.1.7",
  "description": "eslint config for nibfe",
  "author": "pengrui02@meituan.com",
  "scripts": {
    "test": "echo missing test",
    "commit": "git cz",
    "newbranch": "npx git newbranch"
  },
  "devDependencies": {
    "@hfe/common-cli-plugin-flow-standards": "1.0.0",
    "eslint-config-airbnb": "18.0.1",
    "yorkie": "2.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-configrc.js"
    }
  },
  "gitHooks": {
    "pre-commit": "echo empty pre-commit hook",
    "commit-msg": "commitlint -e $GIT_PARAMS",
    "pre-push": "branchlint"
  },
  "keywords": [
    "flow-standards",
    "lint-config",
    "eslint"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "ssh://git@git.sankuai.com/nibfe/eslint-config.git"
  },
  "dependencies": {}
}
